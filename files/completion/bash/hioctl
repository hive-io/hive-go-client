_hioctl_guest()
{
    last_command="hioctl_guest"

    command_aliases=()

    commands=()
    commands+=("add-external")
    commands+=("assign")
    commands+=("backup")
    commands+=("delete")
    commands+=("diff")
    commands+=("get")
    commands+=("list")
    commands+=("list-backups")
    commands+=("migrate")
    commands+=("poweroff")
    commands+=("poweron")
    commands+=("reboot")
    commands+=("refresh")
    commands+=("release")
    commands+=("reset")
    commands+=("restore")
    commands+=("shutdown")
    commands+=("update")
    commands+=("update-external")

    flags=()
    two_word_flags=()
    local_nonpersistent_flags=()
    flags_with_completion=()
    flags_completion=()

