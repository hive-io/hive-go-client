    local_nonpersistent_flags+=("--broker-redirect-usb")
    flags+=("--broker-smart-resize")
    local_nonpersistent_flags+=("--broker-smart-resize")
    flags+=("--enable-ad")
    local_nonpersistent_flags+=("--enable-ad")
    flags+=("--enable-broker-options")
    local_nonpersistent_flags+=("--enable-broker-options")
    flags+=("--enable-uv")
