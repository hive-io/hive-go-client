_hioctl_cluster_reset-broker()
{
    last_command="hioctl_cluster_reset-broker"

    command_aliases=()

    commands=()

    flags=()
    two_word_flags=()
    local_nonpersistent_flags=()
    flags_with_completion=()
    flags_completion=()

    flags+=("--config=")
    two_word_flags+=("--config")
    flags+=("--format=")
    two_word_flags+=("--format")
    flags+=("--host=")
    two_word_flags+=("--host")
    flags+=("--insecure")
    flags+=("-k")
    flags+=("--password=")
    two_word_flags+=("--password")
    two_word_flags+=("-p")
    flags+=("--port=")
    two_word_flags+=("--port")
    flags+=("--realm=")
    two_word_flags+=("--realm")
    two_word_flags+=("-r")
    flags+=("--user=")
    two_word_flags+=("--user")
