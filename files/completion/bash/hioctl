    last_command="hioctl_pool_list"

    command_aliases=()

    commands=()

    flags=()
    two_word_flags=()
    local_nonpersistent_flags=()
    flags_with_completion=()
    flags_completion=()

    flags+=("--count=")
    two_word_flags+=("--count")
    local_nonpersistent_flags+=("--count=")
    flags+=("--details")
    local_nonpersistent_flags+=("--details")
    flags+=("--filter=")
    two_word_flags+=("--filter")
    local_nonpersistent_flags+=("--filter=")
    flags+=("--offset=")
    two_word_flags+=("--offset")
    local_nonpersistent_flags+=("--offset=")
    flags+=("--config=")
    two_word_flags+=("--config")
    flags+=("--format=")
    two_word_flags+=("--format")
    flags+=("--host=")
    two_word_flags+=("--host")
    flags+=("--insecure")
    flags+=("-k")
    flags+=("--password=")
    two_word_flags+=("--password")
    two_word_flags+=("-p")
    flags+=("--port=")
    two_word_flags+=("--port")
    flags+=("--realm=")
    two_word_flags+=("--realm")
    two_word_flags+=("-r")
    flags+=("--user=")
    two_word_flags+=("--user")
    two_word_flags+=("-u")

    must_have_one_flag=()
    must_have_one_noun=()
    noun_aliases=()
}

